main: () => {
    # 测试数组创建
    arr = [1, 2, 3, 4, 5]
    echo "原始数组: " + arr
    
    # 测试数组元素赋值
    arr[0] = 10
    arr[2] = 30
    
    echo "修改后数组: " + arr
    echo "第一个元素: " + arr[0]
    echo "第三个元素: " + arr[2]
    
    # 测试在循环中修改数组
    for (i in range(5)) :
        arr[i] = i * 10

    
    echo "循环修改后: " + arr
}
